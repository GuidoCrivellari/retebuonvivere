<html>
 <head>
  <script src="http://d3js.org/d3.v3.min.js"></script>
 </head>
 <body>
  <div id="pachugo">
   <script>

    d3.csv("g_nodes.csv", function (data) {

     var canvas = d3.select("#pachugo")
     .append("svg");
var chomp = [1, 2, 3, 4];
     canvas.selectAll("rect")
     .data(data)
     .enter()
      .append("rect")
      .attr("width", function(d) {return d.Id * 10;})
      .attr("height", 30)
      .attr("y", function (d, i) {return i * 30;});

     canvas.selectAll("text")
     .data(data)
     .enter()
      .append("text")
      .attr("fill", "red")
      .attr("y", function (d, i) {return i * 30 + 15;})
      .text(function (d) {return d.Id + " " + d.Label;});

     });

   </script>
  </div>
 </body>
<html>
